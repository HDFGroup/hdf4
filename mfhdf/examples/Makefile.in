read_subsets: $(srcdir)/SD_read_subsets.c
get_info: $(srcdir)/SD_get_info.c
find_sds_by_name: $(srcdir)/SD_find_sds_by_name.c
set_get_dim_info: $(srcdir)/SD_set_get_dim_info.c
