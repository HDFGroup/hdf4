bin/depcomp
bin/install-sh
bin/ltmain.sh
bin/missing
bin/test-driver
/configure
hdf/src/h4config.h.in
m4/libtool.m4
m4/ltoptions.m4
m4/ltsugar.m4
m4/ltversion.m4
m4/lt~obsolete.m4

# Other files
/CTestDriver.cmake
/HDF4-C-Trunk.cmake
/atbuild
/autom4te.cache
/build
/build*
/buildat
/buildstatic
/cmake*.sh
/config*.sh
/redeploy*.sh
/CMakeUserPresets.json
HDF4Examples/CMakeUserPresets.json
